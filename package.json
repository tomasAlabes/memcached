    "InnoDB memcached API",
    "cache",
    "client",
    "cluster",
    "failover",
    "hashing",
    "membase",
    "memcache",
    "memcached",
    "nMemcached",
    "nosql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/tomasAlabes/node-memcached.git"
  },
  "dependencies": {
    "hashring": "3.2.x",
    "jackpot": ">=0.0.6"
  },
  "devDependencies": {
    "mocha": "*",
    "should": "*",
    "pre-commit": "*"
  },
  "scripts": {
    "test": "mocha $(find test -name '*.test.js') --exit"
  }
}
